    "@capacitor/keyboard": "^7.0.2",
    "@capacitor/status-bar": "^7.0.2",
    "@ionic/vue": "^8.7.3",
    "@ionic/vue-router": "^8.7.3",
    "core-js": "^3.45.1",
    "ionicons": "^8.0.13",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@commitlint/cli": "^19.8.1",
